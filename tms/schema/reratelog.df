ADD INDEX "UpdateDate" ON "EDRHistory" 
  AREA "Sta_Index_2"
  INDEX-FIELD "Brand" ASCENDING 
  INDEX-FIELD "UpdateDate" DESCENDING 
  INDEX-FIELD "UpdateTime" DESCENDING 

