ADD TABLE "EDRHistory"
  AREA "Sta_Data_64"
  LABEL "EDRHistory"
  DUMP-NAME "edrhistory"

ADD FIELD "CLI" OF "EDRHistory" AS character 
  FORMAT "x(12)"
  INITIAL ""
  LABEL "MSISDN"
  POSITION 2
  MAX-WIDTH 22
  COLUMN-LABEL "MSISDN"
  HELP "MSISDN"
  ORDER 10

ADD FIELD "DateSt" OF "EDRHistory" AS date 
  FORMAT "99-99-99"
