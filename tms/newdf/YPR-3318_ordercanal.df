  ORDER 1010

ADD FIELD "MSISDNForIdent" OF "OrderCustomer" AS Character
  FORMAT "x(12)"
  INITIAL ""
  LABEL "MSISDN For Identification"
  COLUMN-LABEL "MSISDNForIdent"
  ORDER 1020

