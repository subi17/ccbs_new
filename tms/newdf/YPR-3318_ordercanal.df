ADD FIELD "Multiorder" OF "Order" AS logical
  FORMAT "Yes/No"
  INITIAL "No"
  LABEL "Multiorder"
  COLUMN-LABEL "Multiorder"
  ORDER 1170

ADD FIELD "CustDataRetr" OF "OrderCustomer" AS logical
  FORMAT "Yes/No"
  INITIAL "No"
  LABEL "Customer data Retrieved"
