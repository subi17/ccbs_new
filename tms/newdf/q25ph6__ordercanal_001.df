ADD FIELD "ContractID" OF "TermReturn" AS character
  FORMAT "x(12)"
  INITIAL ""  
  COLUMN-LABEL "ContractId"
  LABEL "Contract ID"  
  HELP "Contract ID"
  ORDER 120

ADD FIELD "ReturnChannel" OF "TermReturn" AS character
  FORMAT "x(16)"
  INITIAL ""  
  COLUMN-LABEL "RetChannel"
